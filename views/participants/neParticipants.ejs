
<%- include('../partials/header') %>


<table class="table table-bordered">
    <thead>
      <tr>
        <th scope="col">#</th>
        <th scope="col">Username</th>
        <th scope="col">Name</th>
        <th scope="col">Role</th>
      </tr>
    </thead>
    <% cnt=1 %>
    <tbody>
     <%   p.forEach((x)=>{ %>
            <tr>
                <th scope="row"><%=cnt%></th>
                <td><%=x.user.username%></td>
                <td><%=x.user.name%></td>
                <td><%=x.role%></td>
            </tr>
            <% cnt+=1 %>
      <%  }) %>
    </tbody>
  </table>
  


  <%- include('../partials/footer') %>